{
  "name": "angular.io-example",
  "version": "0.0.0",
  "description": "Example project from an angular.io guide.",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "ng build --configuration=development",
    "build:staging": "ng build --configuration=staging",
    "build:watch": "ng build --watch",
    "build:serve": "lite-server --baseDir='dist'",
    "build:source-map": "ng build --source-map && ls dist/*.js",
    "source-map-explorer": "source-map-explorer",
    "deploy": "ng deploy",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky install",
    "prettier:write": "prettier --write .",
    "prettier:check": "prettier --check .",
    "browserslist": "browserslist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0-next",
    "@angular/common": "^19.2.9",
    "@angular/compiler": "^14.1.0-next",
    "@angular/core": "^14.1.0-next",
    "@angular/fire": "^18.0.1",
    "@angular/forms": "^14.1.0-next",
    "@angular/platform-browser": "^14.1.0-next",
    "@angular/platform-browser-dynamic": "^14.1.0-next",
    "@angular/router": "^14.1.0-next",
    "angular-in-memory-web-api": "~0.14.0",
    "firebase": "^11.0.2",
    "lodash-es": "^4.17.21",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.3",
    "@angular/cli": "~14.0.0",
    "@angular/compiler-cli": "^14.1.0-next",
    "@types/jasmine": "~4.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.35",
    "copyfiles": "^2.4.1",
    "https-proxy-agent": "^5.0.1",
    "husky": "^8.0.1",
    "jasmine-core": "~4.1.0",
    "jasmine-marbles": "~0.9.2",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^15.5.1",
    "lite-server": "^2.6.1",
    "prettier": "2.7.1",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.2",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "browserslist": [
    "defaults"
  ]
}
